<%= form_for([@app, @app.top10s.build]) do |f|%>

<% if @app.errors.any? %>
	<div id="error_explanation">
		<h2>
			<%= pluralize(@app.errors.count, "error")%> prohibited this app for being saved:
		</h2>
		<ul>
			<% @app.errors.full_messages.each do |msg| %>
			<li><%= msg%></li>
			<% end %>
		</ul>
	</div>
	<% end %>


	<p>
		<%= f.label :rank%><br>
		<%= f.number_field :rank %>
	</p>

	<p>
		<%= f.submit%>
	</p>
	<%end%>
