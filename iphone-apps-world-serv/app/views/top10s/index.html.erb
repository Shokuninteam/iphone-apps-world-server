<div class="row">
	<div class="large-1 columns">
		<div class="button secondary">
			<%= link_to 'back', admins_path %>
		</div>
	</div>
	<div class="large-10 columns">
		<h1>All Top 10</h1>
	</div>
</div>
<div class="row">
	<div class="large-5 large-offset-2 columns">
		<table>
			<tr>
				<th>Rank</th>
				<th>Application</th>
				<th colspan="3"></th>
			</tr>
			<%  @top10s.each do |top10| %>

				<tr>
					<td><%= top10.rank%></td>
					<td><%= top10.name%></td>
					<td><%= link_to 'edit', edit_top10_path(top10)%></td>
					<td><%= link_to 'destroy', top10_path(top10.id), method: :delete, data: { confirm:"sure?"}%></td>
				</tr>

			<%end%>
		</table>
	</div>
</div>
