<%= form_for @app do |f|%>
	<p>
		<%= f.label :name%><br>
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :url%><br>
		<%= f.text_field :url%>
	</p>

	<p>
		<%= f.label :price%><br>
		<%= f.number_field :price%>
	</p>
	<p>
		<%= f.label :Description%><br>
		<%= f.text_area :description%>
	</p>

	<p>
		<%= f.label :analysis%><br>
		<%= f.text_area :analysis%>
	</p>
	<h2>listed pros</h2>
	<table>
		<tr>
			<th>description</th>
			<th colspan="2"></th>
		</tr>
		<%= render @app.pros%>
	</table>

	<h2>New pro</h2>
		<%= render 'pros/form' %>

	<p>
		<%= f.submit%>
	</p>
	<%end%>