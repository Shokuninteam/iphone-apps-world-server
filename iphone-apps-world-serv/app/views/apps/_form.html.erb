<%= form_for ([@category, @category.apps.build]) do |f|%>
	<div class="row">
		<div class="large-5 large-offset-2 columns">
			<%= f.label :name%><br>
			<%= f.text_field :name %>
		
			<%= f.label :url%><br>
			<%= f.text_field :url%>
		
			<%= f.label :price%><br>
			<%= f.number_field :price%>
		
			<%= f.label :Description%><br>
			<%= f.text_area :description%>
		
			<%= f.label :analysis%><br>
			<%= f.text_area :analysis%>

			<%= f.submit%>
		</div>
	</div>

	<%end%>